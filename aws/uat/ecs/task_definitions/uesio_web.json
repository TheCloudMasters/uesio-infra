          {
            "name": "UESIO_PLATFORM_DATASOURCE_CREDENTIALS",
            "value": "uesio/core.db"
          },
          {
            "name": "UESIO_PLATFORM_FILESOURCE_TYPE",
            "value": "uesio.s3"
